<div class="containerProd" id="container">
{{#each users}} 
<div class="product-card cardProd">
  <span class="oculto" id="idOculto">{{this._id}}</span>
  <h2 class="product-card-title" id="titulo">Nombre: {{this.name}}</h2>
  <span class="product-card-desc">Apellido: {{this.last_name}}</span>
  <span class="product-card-price">Email: {{this.email}}</span>
  <br>
  <span class="product-card-price">Tipo de usuario: {{this.rol}}</span>
  <button  class="btn-mostrar" data-id={{this._id}}>
    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-pencil" viewBox="0 0 16 16">
  <path d="M12.146.146a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1 0 .708l-10 10a.5.5 0 0 1-.168.11l-5 2a.5.5 0 0 1-.65-.65l2-5a.5.5 0 0 1 .11-.168l10-10zM11.207 2.5 13.5 4.793 14.793 3.5 12.5 1.207 11.207 2.5zm1.586 3L10.5 3.207 4 9.707V10h.5a.5.5 0 0 1 .5.5v.5h.5a.5.5 0 0 1 .5.5v.5h.293l6.5-6.5zm-9.761 5.175-.106.106-1.528 3.821 3.821-1.528.106-.106A.5.5 0 0 1 5 12.5V12h-.5a.5.5 0 0 1-.5-.5V11h-.5a.5.5 0 0 1-.468-.325z"/>
</svg>
</button>
<div class="hide ocultos" id={{this._id}}>
<input type="text" id="id{{this._id}}">
<button class="btn-actualizar" data-id={{this._id}}>Actualizar</button>
</div>
  <br>
  <button class="btn-borrar" data-id={{this._id}}><span>Borrar</span></button>

</div> 
{{/each}}
</div>

<script src="/js/administrarUsers.js"></script>

