<nav class="nav">
    <div class="nav-user">
        <button><a href="/">Iniciar sesion</a></button>
        <button><a href="/register">Registrarse</a></button>
        <button style="display: none;" id="logout" class="btn-logout">Logout</button>
        <button style="display: none;" id="goCarrito">Carrito</button>
        <button style="display: none;"><a href="/administrar">Administrar productos</a></button>
    </div>
</nav>

<div class="containerProd">
{{#each prods}}
<div class="product-card cardProd">
  <div class="product-card-thumbnail container-img">
    <img src={{this.thumbnail}} />
  </div>
  <span class="oculto">{{this._id}}</span>
  <h2 class="product-card-title" id="titulo">{{this.title}}</h2>
  <span class="product-card-desc">{{this.description}}</span>
  <span class="product-card-price">${{this.price}}</span>
  <br>
  <span class="product-card-price">Stock: {{this.stock}}</span>
  <br>
  <button style="display: none;" class="button-hover-addcart button agregarCarritoC" id="agregarCarrito" onclick="addCarrito('{{this._id}}')"><span>Add</span><i class="fa fa-shopping-cart"></i></button>

</div>
{{/each}}
</div>
    <script src="/js/products.js"></script>